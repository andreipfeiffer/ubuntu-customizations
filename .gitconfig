[core]
  excludefile = ~/.gitignore
  quotepath = false
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
[color]
  ui = auto
[color "branch"]
  current = green
  local = yellow
  remote = magenta
[color "diff"]
  meta = yellow
  frag = magenta
  old = red
  new = green
  whitespace = white reverse
[color "status"]
  added = green
  changed = yellow
  untracked = magenta
  branch = red
